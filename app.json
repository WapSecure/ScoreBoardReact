{
  "expo": {
    "name": "Pengscores",
    "slug": "Pengscores",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3353dd"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
