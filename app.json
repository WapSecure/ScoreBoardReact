    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.olamilekan.pengscores"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#3353dd"
    }
  }
}